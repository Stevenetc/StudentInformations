@model List<StudentInformation.Models.Student>

<h1>Student List</h1>
<table class="table">
    <thead>
        <tr>
            <th>StudentID</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Gender</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.StudentID</td>
                <td>@student.FirstName</td>
                <td>@student.MiddleName</td>
                <td>@student.LastName</td>
                <td>@student.Gender</td>
                <td>@student.Age</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@student.StudentID">Edit</a> |
                    <a asp-action="Delete" asp-route-id=@student.StudentID>Delete</a>
                </td>
            </tr>
        }
    </tbody>

</table>

<a href="#"><a asp-action="RegisterStudent" class=" btn btn-primary">Register Student</a>